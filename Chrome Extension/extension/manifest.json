{
  "name": "Hands Free Controller",
  "description": "Use your webcam to interact with websites",
  "version": "1.0",
  "manifest_version": 3,
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "/weights/*"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ]
}
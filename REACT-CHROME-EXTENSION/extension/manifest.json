{
  "name": "Hands Free Controller",
  "description": "Use your webcam to interact with websites",
  "version": "1.0.0",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "/weights/*"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ]
}